<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#232323"
    tools:context=".NutritionActivity">

    <ImageButton
        android:id="@+id/btnUpdateProfile"
        android:layout_width="48dp"
        android:layout_height="60dp"
        android:layout_marginTop="36dp"
        android:layout_marginEnd="48dp"
        android:background="#00FFFFFF"
        android:contentDescription="Update Profile"
        android:scaleType="centerCrop"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/editprof" />

    <TextView
        android:id="@+id/tvNutrition"
        android:layout_width="137dp"
        android:layout_height="29dp"
        android:layout_marginStart="38dp"
        android:layout_marginTop="52dp"
        android:layout_marginEnd="173dp"
        android:fontFamily="@font/poppins_semibold"
        android:text="Nutrition"
        android:textColor="#896CFE"
        android:textSize="20sp"
        app:layout_constraintEnd_toStartOf="@+id/btnUpdateProfile"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/imageButton"
        android:layout_width="323dp"
        android:layout_height="110dp"
        android:background="#00FFFFFF"
        android:scaleType="centerCrop"
        app:layout_constraintBottom_toTopOf="@+id/imageButton4"
        app:layout_constraintTop_toBottomOf="@+id/btnUpdateProfile"
        app:layout_constraintVertical_bias="0.5"
        app:srcCompat="@drawable/food5"
        tools:layout_editor_absoluteX="44dp" />

    <ImageButton
        android:id="@+id/imageButton4"
        android:layout_width="323dp"
        android:layout_height="110dp"
        android:background="#00FFFFFF"
        android:scaleType="centerCrop"
        app:layout_constraintBottom_toTopOf="@+id/imageButton5"
        app:layout_constraintTop_toBottomOf="@+id/imageButton"
        app:layout_constraintVertical_bias="0.5"
        app:srcCompat="@drawable/food2"
        tools:layout_editor_absoluteX="44dp" />

    <ImageButton
        android:id="@+id/imageButton5"
        android:layout_width="323dp"
        android:layout_height="110dp"
        android:background="#00FFFFFF"
        android:scaleType="centerCrop"
        app:layout_constraintBottom_toTopOf="@+id/imageButton6"
        app:layout_constraintTop_toBottomOf="@+id/imageButton4"
        app:layout_constraintVertical_bias="0.5"
        app:srcCompat="@drawable/food3"
        tools:layout_editor_absoluteX="44dp" />

    <ImageButton
        android:id="@+id/imageButton6"
        android:layout_width="323dp"
        android:layout_height="110dp"
        android:background="#00FFFFFF"
        android:scaleType="centerCrop"
        app:layout_constraintBottom_toTopOf="@+id/imageButton8"
        app:layout_constraintTop_toBottomOf="@+id/imageButton5"
        app:layout_constraintVertical_bias="0.5"
        app:srcCompat="@drawable/food4"
        tools:layout_editor_absoluteX="44dp" />

    <ImageButton
        android:id="@+id/imageButton8"
        android:layout_width="323dp"
        android:layout_height="110dp"
        android:background="#00FFFFFF"
        android:scaleType="centerCrop"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageButton6"
        app:layout_constraintVertical_bias="0.5"
        app:srcCompat="@drawable/food1"
        tools:layout_editor_absoluteX="44dp" />
</androidx.constraintlayout.widget.ConstraintLayout>