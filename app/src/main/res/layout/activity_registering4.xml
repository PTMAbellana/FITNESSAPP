<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#232323"
    tools:context=".registering.Registering4Activity">

    <Button
        android:id="@+id/btnBack"
        android:layout_width="108dp"
        android:layout_height="57dp"
        android:layout_marginStart="4dp"
        android:layout_marginTop="16dp"
        android:background="#00E2F163"
        android:text="Back"
        android:onClick="onBackClicked"
        android:textColor="#E2F163"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <SeekBar
        android:id="@+id/seekBar2"
        style="@style/Widget.AppCompat.SeekBar.Discrete"
        android:layout_width="203dp"
        android:layout_height="30dp"
        android:layout_marginTop="64dp"
        android:max="4"
        android:progress="0"
        android:progressTint="@color/colorWhite"
        android:thumbTint="@color/colorWhite"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tvChooseNoOfPushUps"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="28dp"
        android:fontFamily="@font/poppins_semibold"
        android:paddingLeft="30dp"
        android:paddingRight="30dp"
        android:text="How many push-ups can you do at one time"
        android:textAlignment="center"
        android:textColor="#E2F163"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/seekBar2" />

    <FrameLayout
        android:id="@+id/frameChoosePlan1"
        android:layout_width="300dp"
        android:layout_height="80dp"
        android:layout_marginStart="55dp"
        android:layout_marginTop="28dp"
        android:layout_marginEnd="56dp"
        android:background="@drawable/rounded_frame_layout"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tvChooseNoOfPushUps">

        <TextView
            android:id="@+id/messageChoosePlan1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center|left"
            android:fontFamily="@font/poppins_semibold"
            android:paddingLeft="20dp"
            android:paddingRight="20dp"
            android:text="I can't do it."
            android:textSize="16sp" />

        <ImageView
            android:id="@+id/imageView1"
            android:layout_width="25dp"
            android:layout_height="25dp"
            android:layout_gravity="center|right"
            android:layout_marginRight="20dp"
            app:srcCompat="@drawable/circle_shape" />

    </FrameLayout>


    <FrameLayout
        android:id="@+id/frameChoosePlan2"
        android:layout_width="300dp"
        android:layout_height="80dp"
        android:layout_marginTop="16dp"
        android:background="@drawable/rounded_frame_layout"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/frameChoosePlan1">

        <TextView
            android:id="@+id/messageChoosePlan2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center|left"
            android:fontFamily="@font/poppins_semibold"
            android:paddingLeft="20dp"
            android:paddingRight="20dp"
            android:text="5-10 repeats"
            android:textSize="16sp" />

        <ImageView
            android:id="@+id/imageView2"
            android:layout_width="25dp"
            android:layout_height="25dp"
            android:layout_gravity="center|right"
            android:layout_marginRight="20dp"
            app:srcCompat="@drawable/circle_shape" />

    </FrameLayout>

    <FrameLayout
        android:id="@+id/frameChoosePlan3"
        android:layout_width="300dp"
        android:layout_height="80dp"
        android:layout_marginStart="55dp"
        android:layout_marginTop="16dp"
        android:background="@drawable/rounded_frame_layout"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/frameChoosePlan2">

        <TextView
            android:id="@+id/messageChoosePlan3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center|left"
            android:fontFamily="@font/poppins_semibold"
            android:paddingLeft="20dp"
            android:paddingRight="20dp"
            android:text="0-5 repeats"
            android:textSize="16sp" />

        <ImageView
            android:id="@+id/imageView3"
            android:layout_width="25dp"
            android:layout_height="25dp"
            android:layout_gravity="center|right"
            android:layout_marginRight="20dp"
            app:srcCompat="@drawable/circle_shape" />

    </FrameLayout>

    <FrameLayout
        android:id="@+id/frameChoosePlan4"
        android:layout_width="300dp"
        android:layout_height="80dp"
        android:layout_marginTop="16dp"
        android:background="@drawable/rounded_frame_layout"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/frameChoosePlan3">

        <TextView
            android:id="@+id/messageChoosePlan4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center|left"
            android:fontFamily="@font/poppins_semibold"
            android:paddingLeft="20dp"
            android:paddingRight="20dp"
            android:text="10-20 repeats"
            android:textSize="16sp" />

        <ImageView
            android:id="@+id/imageView4"
            android:layout_width="25dp"
            android:layout_height="25dp"
            android:layout_gravity="center|right"
            android:layout_marginRight="20dp"
            app:srcCompat="@drawable/circle_shape" />

    </FrameLayout>

    <FrameLayout
        android:id="@+id/frameChoosePlan5"
        android:layout_width="300dp"
        android:layout_height="80dp"
        android:layout_marginTop="16dp"
        android:background="@drawable/rounded_frame_layout"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/frameChoosePlan4">

        <TextView
            android:id="@+id/messageChoosePlan5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center|left"
            android:fontFamily="@font/poppins_semibold"
            android:paddingLeft="20dp"
            android:paddingRight="20dp"
            android:text="Over 20 repeats"
            android:textSize="16sp" />

        <ImageView
            android:id="@+id/imageView5"
            android:layout_width="25dp"
            android:layout_height="25dp"
            android:layout_gravity="center|right"
            android:layout_marginRight="20dp"
            app:srcCompat="@drawable/circle_shape" />

    </FrameLayout>

</androidx.constraintlayout.widget.ConstraintLayout>