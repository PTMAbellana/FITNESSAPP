<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#232323"
    tools:context=".WorkoutActivity">

    <ImageButton
        android:id="@+id/btnUpdateProfile"
        android:layout_width="48dp"
        android:layout_height="60dp"
        android:layout_marginTop="36dp"
        android:layout_marginEnd="48dp"
        android:background="#00FFFFFF"
        android:contentDescription="Update Profile"
        android:scaleType="centerCrop"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/editprof" />

    <TextView
        android:id="@+id/tvGreeting"
        android:layout_width="137dp"
        android:layout_height="29dp"
        android:layout_marginStart="38dp"
        android:layout_marginTop="52dp"
        android:layout_marginEnd="173dp"
        android:fontFamily="@font/poppins_semibold"
        android:text="Workout"
        android:textColor="#896CFE"
        android:textSize="20sp"
        app:layout_constraintEnd_toStartOf="@+id/btnUpdateProfile"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="323dp"
        android:layout_height="110dp"
        android:layout_marginTop="19dp"
        android:layout_marginBottom="22dp"
        app:layout_constraintBottom_toTopOf="@+id/imageView7"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnVideo1"
        app:srcCompat="@drawable/video3" />

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="323dp"
        android:layout_height="110dp"
        android:layout_marginTop="22dp"
        android:layout_marginBottom="187dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView6"
        app:srcCompat="@drawable/video4" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="323dp"
        android:layout_height="110dp"
        android:layout_marginTop="12dp"
        android:layout_marginBottom="15dp"
        app:layout_constraintBottom_toTopOf="@+id/btnVideo1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout5"
        app:srcCompat="@drawable/video1" />

    <LinearLayout
        android:id="@+id/linearLayout5"
        android:layout_width="323dp"
        android:layout_height="85dp"
        android:layout_marginStart="44dp"
        android:layout_marginTop="15dp"
        android:layout_marginEnd="44dp"
        android:gravity="center"
        android:orientation="horizontal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnUpdateProfile">

        <Button
            android:id="@+id/beginnerbtn"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="5dp"
            android:fontFamily="@font/poppins_semibold"
            android:padding="5dp"
            android:text="Beginner"
            android:textSize="13sp" />

        <Button
            android:id="@+id/intermediateBtn"
            android:layout_width="115dp"
            android:layout_height="wrap_content"
            android:layout_margin="5dp"
            android:layout_weight="1"
            android:fontFamily="@font/poppins_semibold"
            android:padding="5dp"
            android:text="Intermediate"
            android:textSize="13sp" />

        <Button
            android:id="@+id/advancedBtn"
            android:layout_width="89dp"
            android:layout_height="wrap_content"
            android:layout_margin="5dp"
            android:layout_weight="1"
            android:fontFamily="@font/poppins_semibold"
            android:padding="5dp"
            android:text="Advanced"
            android:textSize="13sp" />
    </LinearLayout>

    <ImageView
        android:id="@+id/btnVideo1"
        android:layout_width="323dp"
        android:layout_height="110dp"
        android:layout_marginTop="9dp"
        android:layout_marginBottom="8dp"
        app:layout_constraintBottom_toTopOf="@+id/imageView6"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView2"
        app:srcCompat="@drawable/video2" />

    <ImageButton
        android:id="@+id/btnVideo2"
        android:layout_width="18dp"
        android:layout_height="18dp"
        android:layout_marginBottom="873dp"
        android:background="#00FFFFFF"
        android:scaleType="centerCrop"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/imageView2"
        app:layout_constraintStart_toEndOf="@+id/imageView2"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/playbtn" />

    <ImageButton
        android:id="@+id/btnVideo3"
        android:layout_width="18dp"
        android:layout_height="18dp"
        android:layout_marginBottom="873dp"
        android:background="#00FFFFFF"
        android:scaleType="centerCrop"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/btnVideo1"
        app:layout_constraintStart_toEndOf="@+id/btnVideo1"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/playbtn" />

    <ImageButton
        android:id="@+id/btnVideo4"
        android:layout_width="48dp"
        android:layout_height="48dp"
        android:layout_marginTop="642dp"
        android:layout_marginBottom="231dp"
        android:background="#00FFFFFF"
        android:scaleType="centerCrop"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/imageView7"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/playbtn"
        tools:ignore="DuplicateIds" />

    <ImageButton
        android:id="@+id/imageButton7"
        android:layout_width="18dp"
        android:layout_height="18dp"
        android:layout_marginStart="42dp"
        android:layout_marginEnd="42dp"
        android:background="#00FFFFFF"
        android:scaleType="centerCrop"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/imageView6"
        app:layout_constraintHorizontal_bias="0.574"
        app:layout_constraintStart_toEndOf="@+id/imageView6"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.018"
        app:srcCompat="@drawable/playbtn" />

    <ImageButton
        android:id="@+id/btnVideo4"
        android:layout_width="18dp"
        android:layout_height="18dp"
        android:layout_marginStart="42dp"
        android:layout_marginEnd="42dp"
        android:background="#00FFFFFF"
        android:scaleType="centerCrop"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/imageView6"
        app:layout_constraintStart_toEndOf="@+id/imageView6"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/playbtn"
        tools:ignore="DuplicateIds" />

</androidx.constraintlayout.widget.ConstraintLayout>